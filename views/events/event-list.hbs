<h1>Event List Page</h1>

<div class="event-container">

{{#each events}}
    <div class="event-card">
         <h2>{{eventName}}</h2>
         <img src="{{image}}" alt="">
        <h5>Description: {{description}}</h5>
        <a href="/events/{{_id}}">More Details</a>
        <form id="delete-form" action="/delete" method="POST">
    <button>Delete Event</button>
  </form>
        
    </div>
    <br>
{{/each}}

</div>